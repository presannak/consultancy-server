{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "redirects": [
        {
          "source": "/reviews",
          "destination": "/"
        }
      ],
    "rewrites": [
        {
          "source": "/reviews",
          "destination": "/"
        }
      ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods":["GET","POST","PUT","PATCH","DELETE","OPTIONS"]
      }
    ]
}